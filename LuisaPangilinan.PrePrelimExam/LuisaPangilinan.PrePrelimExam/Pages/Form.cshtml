@page
@model LuisaPangilinan.PrePrelimExam.Pages.FormModel
@{
}
<div class="card" style="width: 18rem;">
    <h1>@Model.Data?.Weather?.FirstOrDefault()?.Main</h1>
    <p>@Model.Data?.Weather?.FirstOrDefault()?.Description</p>
    <ul>
        <li>Title : @Model.Data?.Main?.Temp</li>
        <li>Title : @Model.Data?.Main?.FeelsLike</li>
        <li>Title : @Model.Data?.Main?.Humidity</li>
        <li>Title : @Model.Data?.Main?.Pressure</li>
    </ul>
</div>
<form method="post">
    <div class="form-group">
        <label asp-for="Book">Book title</label>
        <input type="text" class="form-control" id="@Model.Book?.Title" placeholder="Title">
    </div>
    <div class="form-group">
        <label asp-for="Book">Author</label>
        <input type="text" class="form-control" id="@Model.Book?.Author" placeholder="author">
    </div>
    <div class="form-group">
        <label asp-for="Book">PublishDate</label>
        <input type="text" class="form-control" id="@Model.Book?.PublishDate" placeholder="PublishDate">
    </div>
    <div class="form-group">
        <label asp-for="Book">description</label>
        <input type="text" class="form-control" id="@Model.Book?.Description" placeholder="Description">
    </div>
    <div class="form-group">
        <label asp-for="Book">genre</label>
        <input type="text" class="form-control" id="@Model.Book?.Genre" placeholder="Genre">
    </div>
    <button formmethod="post" type="submit" class="btn">Submit</button>
</form>